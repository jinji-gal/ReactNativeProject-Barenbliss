{
  "expo": {
    "name": "barenbliss",
    "slug": "barenbliss",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.donnabaldoza.barenbliss"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.donnabaldoza.barenbliss"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      "expo-secure-store",
      "expo-font"
    ],
    "scheme": "barenbliss",
    "facebookScheme": "fb1196596965367294",
    "owner": "donn_baldoza",
    "extra": {
      "googleClientId": "562957089179-v0glkbdo2sc169prvf84hhrdi0p2rouj.apps.googleusercontent.com",
      "facebookAppId": "1196596965367294",
      "eas": {
        "projectId": "7a47c849-db2e-4dba-af08-01a6f8497072"
      }
    }
  }
}
